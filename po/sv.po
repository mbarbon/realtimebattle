#: src/RealTimeBattle.cc:91
msgid " Usage: RealTimeBattle [options] "
msgstr " Användning: RealTimeBattle [flaggor] "

#: src/RealTimeBattle.cc:92
msgid " Options:"
msgstr " Flaggor:"

#: src/RealTimeBattle.cc:93
msgid "    --debug_mode,                -d   debug mode"
msgstr "    --debug_mode,                -d   debugläge"

#: src/RealTimeBattle.cc:94
msgid ""
"    --debug_level [0-5],         -D   sets the initial debug level. implies "
"-d"
msgstr ""
"    --debug_level [0-5],         -D   sätt debug-nivå. Implicerar -d"

#: src/RealTimeBattle.cc:95
msgid "    --normal_mode,               -n   normal mode (default)"
msgstr "    --normal_mode,               -n   normalt läge (default)"

#: src/RealTimeBattle.cc:96
msgid "    --competition_mode,          -c   competition mode"
msgstr "    --competition_mode,          -c   tävlingsläge"

#: src/RealTimeBattle.cc:98
msgid "    --no_graphics,               -g   no graphics will be displayed"
msgstr "    --no_graphics,               -g   visa ingen grafik"

#: src/RealTimeBattle.cc:99
msgid ""
"    --option_file [file],        -o   selects option-file (default: "
"$HOME/.rtbrc)"
msgstr ""
"    --option_file [fil],         -o   välj inställningsfil (default: "
"$HOME/.rtbrc)"

#: src/RealTimeBattle.cc:101
msgid "    --log_file [file],           -l   make log file, if 'file' is '-'"
msgstr "    --log_file [fil],            -l   skapa log-file, om 'fil' är '-'"

#: src/RealTimeBattle.cc:102
msgid "                                      the log is sent to STDOUT"
msgstr "                                      så skickas loggen till STDOUT"

#: src/RealTimeBattle.cc:104
msgid "    --tournament_file [file],    -t   specify a tournament file to"
msgstr "    --tournament_file [fil],     -t   välj turneringsfil för automatisk"

#: src/RealTimeBattle.cc:105
msgid "                                      autostart a tournament"
msgstr "                                      start av turneringen"

#: src/RealTimeBattle.cc:107
msgid "    --statistics_file [file],    -s   file to print the statistics to"
msgstr "    --statistics_file [fil],     -s   skriv statistik till fil vid"

#: src/RealTimeBattle.cc:108
msgid "                                       when autostarting"
msgstr "                                      automatisk turneringsstart"

#: src/RealTimeBattle.cc:110
msgid "    --message_file [file],       -m   redirect messages to 'file'."
msgstr "    --message_file [fil],        -m   skicka meddelanden till 'fil'"

#: src/RealTimeBattle.cc:111
msgid ""
"                                      '-' as 'file' is equivalent to STDOUT."
msgstr ""
"                                      Om 'fil' är '-' används STDOUT."
#: src/RealTimeBattle.cc:112
msgid "                                      If both log and messages are send"
msgstr "                                      Om både loggen och meddelanden skickas"

#: src/RealTimeBattle.cc:113
msgid "                                      to STDOUT, '-m' will be ignored"
msgstr "                                      till STDOUT kommer '-m' att ignoreras"

#: src/RealTimeBattle.cc:115
msgid "    --replay [file]              -r   a log file to replay."
msgstr "    --replay [fil]               -r   visa spel sparat i 'fil'."

#: src/RealTimeBattle.cc:116
msgid "                                      if '-' is specified as file,"
msgstr "                                      om filen är '-' kommer"

#: src/RealTimeBattle.cc:117
msgid "                                      input is taken from STDIN"
msgstr "                                      STDIN användas"

#: src/RealTimeBattle.cc:119
